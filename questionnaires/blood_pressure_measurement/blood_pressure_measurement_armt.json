        "select_choices_or_calculations": [
            {
                "code": "1",
                "label": "• Blood Pressure Measuring instructions:"
            },
            {
                "code": "2",
                "label": "• Please sit quietly for 5 minutes, with your back supported and feet on the floor."
            },
            {
                "code": "3",
                "label": "• Support the arm, so that the cuff will be at the same level as the heart (e.g. table)."
            },
            {
                "code": "4",
                "label": "• The cuff should be positioned 2-3cm over the elbow; the air tube should run down the inside of the forearm and should be in line with the middle finger."
            },
            {
                "code": "5",
                "label": "• The cuff is suited for an arm circumference of 22-42cm."
            },
            {
                "code": "6",
                "label": "• Make 2 repeated recordings (5 minutes between the 2 recordings) over the right arm."
            },
            {
                "code": "7",
                "label": "• Report the values of the 2 recordings of systolic and diastolic measurements on this app."
            },
            {
                "code": "8",
                "label": "• Note the heart rate at the second recording."
            },
            {
                "code": "9",
                "label": "• For further instructions, please see your Technology User Guide or contact the research team."
            }
        ],
