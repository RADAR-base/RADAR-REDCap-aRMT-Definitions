  {
    "field_name": "bpressure_instructions",
    "form_name": "blood_pressure_measurement",
    "section_header": "",
    "field_type": "info",
    "field_label": "",
    "select_choices_or_calculations": [
      {
        "code": "1",
        "label": "• Instrucciones para medir la presión arterial:"
      },
      {
        "code": "2",
        "label": "• Siéntese en silencio durante 5 minutos, con la espalda apoyada y los pies en el suelo."
      },
      {
        "code": "3",
        "label": "• Sostenga su brazo para que el manguito esté al mismo nivel que su corazón (p. ej., una mesa)."
      },
      {
        "code": "4",
        "label": "• El manguito debe colocarse 2-3 cm por encima del codo; el tubo de aire debe correr por el interior del antebrazo y debe estar alineado con el dedo medio."
      },
      {
        "code": "5",
        "label": "• El manguito es adecuado para una circunferencia de brazo de 22-42cm."
      },
      {
        "code": "6",
        "label": "• Realice 2 registros repetidos (5 minutos entre los 2 registros) en el brazo derecho."
      },
      {
        "code": "7",
        "label": "• Anote los valores de los 2 registros de las medidas sistólica y diastólica en esta aplicación."
      },
      {
        "code": "8",
        "label": "• Registre la frecuencia cardíaca en el segundo registro."
      },
      {
        "code": "9",
        "label": "• Para obtener más información, consulte la Guía técnica del usuario o comuníquese con el equipo de investigación."
      }
    ],
